map r
map ra rename
map rr push ra<c-u>
map rb push ra<c-a>

map <enter> open

map a
map aa extract

map d
map dd cut
map dD delete
map dt trash
map <delete> delete

map y
map yy copy

map p
map pp paste

map c
map cd push $mkdir<space>-p<space>
map cf push $touch<spcae>

map u
map uu clear
map uv unselect

map i
map i $bat --paging=always -p "$f"

map gm cd /mnt/ext/
map gw cd ~/Work/Sprints/
map gc cd ~/.config/
map gd cd ~/.dotfiles/
map go cd ~/opt/
map gb cd ~/Study/computer-science-engineering/books/
map gl cd ~/Study/computer-science-engineering/learning/
map gt cd ~/Downloads/torrent/
map gf cd ~/Downloads/firefox/

map S push w

cmap <tab> cmd-menu-complete
cmap <backtab> cmd-menu-complete-back

cmap <up> cmd-history-prev
cmap <down> cmd-history-next
